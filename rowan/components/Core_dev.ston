			},
			'tests' : {
				#packageNames : [
					'FileDescriptor-Tests',
					'FileSystem-Tests-Attributes',
					'FileSystem-Tests-Core',
					'FileSystem-Tests-Disk',
					'FileSystem-Tests-Memory',
					'Zinc-Character-Encoding-Tests',
					'Files-Tests',
					'LibC-Tests',
					'Network-UUID-Tests',
					'FileSystem-Tests-GemStone'
				]
			}
